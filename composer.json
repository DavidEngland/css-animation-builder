{
    "name": "reia/css-animation-builder",
    "description": "A standalone CSS Animation Builder library for creating beautiful animations with interactive controls - developed by Real Estate Intelligence Agency (REIA)",
    "version": "1.6.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "David England",
            "email": "DavidEngland@hotmail.com",
            "homepage": "https://github.com/DavidEngland",
            "role": "Senior Lead Developer"
        },
        {
            "name": "Mikko P. Jetsu",
            "email": "Mikko@RealEstate-Huntsville.com",
            "role": "CEO & Chief Broker"
        }
    ],
    "keywords": [
        "css",
        "animation",
        "builder",
        "generator",
        "interactive",
        "javascript",
        "html",
        "frontend"
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Reia\\CSSAnimationBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Reia\\CSSAnimationBuilder\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
