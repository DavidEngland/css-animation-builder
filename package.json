{
  "name": "@reia/css-animation-builder",
  "version": "1.3.0",
  "description": "A standalone, framework-agnostic CSS Animation Builder library with interactive interface - developed by Real Estate Intelligence Agency (REIA)",
  "main": "dist/css-animation-builder.js",
  "module": "dist/css-animation-builder.esm.js",
  "unpkg": "dist/css-animation-builder.min.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "assets",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint assets/js --ext .js",
    "lint:fix": "eslint assets/js --ext .js --fix",
    "format": "prettier --write assets/js/**/*.js",
    "docs": "typedoc assets/js --out docs",
    "clean": "rimraf dist",
    "prepare": "npm run clean && npm run build"
  },
  "keywords": [
    "css",
    "animation",
    "builder",
    "interactive",
    "wordpress",
    "standalone",
    "framework-agnostic",
    "web-animations",
    "ui-builder"
  ],
  "author": "David England <DavidEngland@hotmail.com> (https://github.com/DavidEngland)",
  "contributors": [
    {
      "name": "Mikko P. Jetsu",
      "email": "Mikko@RealEstate-Huntsville.com",
      "role": "CEO & Chief Broker"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DavidEngland/css-animation-builder.git"
  },
  "bugs": {
    "url": "https://github.com/DavidEngland/css-animation-builder/issues"
  },
  "homepage": "https://github.com/DavidEngland/css-animation-builder#readme",
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.22.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.3",
    "babel-jest": "^29.5.0",
    "eslint": "^8.42.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-dom": "^4.0.0",
    "jest-environment-jsdom": "^29.5.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "rimraf": "^5.0.1",
    "rollup": "^3.25.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.63.0",
    "typedoc": "^0.24.0"
  },
  "dependencies": {},
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}